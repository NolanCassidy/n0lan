{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src/**/*.ts" /* Include every ts file in source folder */
  ],
  "exclude": [
    "node_modules" /* exclude everything in  node_modules */
  ]
}
